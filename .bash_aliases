# Misc Aliases
alias q=exit
# Opens command in a new terminal 
# E.g. "launch gotop" launches gotop in a new terminal which closes when the process ends
alias launch="xfce4-terminal -e"

# LS Aliases
alias lc="ls --color=no" # I use XFCE4 terminal that is transparent, so sometimes the output is hard on the eyes to read
alias lca="lc -a --color=no"
alias la="ls -a"
